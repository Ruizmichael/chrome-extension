{
    "manifest_version": 2,
    "name": "Dark Mode",
    "version": "1.0",
    "description": "Makes things dark",
    "background": {
        "'scripts": [
            "main.js"
        ]
    },
    "browser_action": {
        "default_title": "Dark Mode",
        "default_popup": "optionsPopup.html",
        "default_icon": {
        "48": "/assets/darkmode.png",
        "128": "/assets/darkmode.png"
        }
      },
    "icons": {
        "48": "/assets/darkmode.png",
        "128": "/assets/darkmode.png"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}